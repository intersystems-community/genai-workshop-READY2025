{
	"name": "Jupyter Dev with IRIS",
	"dockerComposeFile": "../docker-compose.yml",
	"shutdownAction": "stopCompose",
	"service": "jupyter",
	"workspaceMount": "source=${localWorkspaceFolder},target=/home/jovyan/notebooks,type=bind,consistency=cached",
	"workspaceFolder": "/home/jovyan/notebooks",
	"forwardPorts": [8889,"iris:52773","iris:1972"],
	"vscode": {
		"extensions": [
		  "ms-toolsai.jupyter",
		  "ms-python.python",
		  "vscode-default-python-kernel"
		]
		
	  }
  }